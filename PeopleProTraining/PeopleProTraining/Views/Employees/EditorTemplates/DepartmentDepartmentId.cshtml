@model int?


<div class="form-group">
    @Html.LabelFor(model => model, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownListFor(t=> this.Model , (IEnumerable<SelectListItem>)ViewBag.DepartmentDepartmentId, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model, string.Empty, new { @class = "text-danger" })
    </div>
</div>
